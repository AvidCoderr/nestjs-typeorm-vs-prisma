{
  "name": "fulleststack",
  "version": "0.0.1",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx lint",
    "pre-commit": "ls ",
    "<--------- [ TypeORM ] ------->": "",
    "typeorm": "TS_NODE_COMPILER_OPTIONS='{\"experimentalDecorators\": true, \"strictPropertyInitialization\": false, \"emitDecoratorMetadata\": true}' node --require ts-node/register ./node_modules/typeorm/cli.js -d ./apps/app-api/db/typeorm/orm.cli.cfg.ts",
    "orm:gen": "yarn typeorm migration:generate apps/app-api/db/typeorm/migrations/M",
    "orm:run": "yarn typeorm migration:run",
    "orm:show": "yarn typeorm migration:show",
    "orm:revert": "yarn  typeorm migration:revert",
    "<--------- [ Prisma ] ------->": "",
    "prisma:gen": "node ./node_modules/.bin/prisma migrate dev --create-only --schema=apps/app-api/db/prisma/schema.prisma",
    "prisma:run": "node ./node_modules/.bin/prisma migrate deploy --schema=apps/app-api/db/prisma/schema.prisma",
    "prisma:studio": "node ./node_modules/.bin/prisma studio --schema=apps/app-api/db/prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.6",
    "@nestjs/common": "9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "9.0.0",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "@prisma/client": "^4.7.1",
    "bcrypt": "^5.1.0",
    "husky": "8.0.2",
    "pg": "^8.8.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.6.0",
    "sqlite3": "^5.1.2",
    "ts-essentials": "^9.3.0",
    "tslib": "2.3.0",
    "typeorm": "0.3.11"
  },
  "devDependencies": {
    "@nestjs/schematics": "9.0.0",
    "@nestjs/testing": "9.0.0",
    "@nrwl/cli": "15.2.4",
    "@nrwl/eslint-plugin-nx": "15.2.4",
    "@nrwl/jest": "15.2.4",
    "@nrwl/linter": "15.2.4",
    "@nrwl/nest": "15.2.4",
    "@nrwl/node": "15.2.4",
    "@nrwl/workspace": "15.2.4",
    "@types/jest": "28.1.1",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "nx": "15.2.4",
    "prettier": "^2.8.0",
    "prisma": "^4.7.1",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typeorm-seeding": "^1.6.1",
    "typescript": "4.8.2"
  }
}
