{
  "name": "store-js",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/store-js/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/store-js",
        "main": "libs/store-js/src/index.ts",
        "tsConfig": "libs/store-js/tsconfig.lib.json",
        "assets": ["libs/store-js/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/store-js/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/store-js/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["lib:store-js", "scope:data-util"]
}
